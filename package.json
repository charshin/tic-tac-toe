{
  "name": "tic-tac-toe",
  "version": "0.1.0",
  "description": "Tic Tac Toe",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development PLATFORM=browser webpack-dev-server --env.mode development --hot",
    "build": "cross-env NODE_ENV=production PLATFORM=browser webpack --env.mode production",
    "serve": "cross-env NODE_ENV=production PLATFORM=node node server",
    "build:dev": "cross-env NODE_ENV=development PLATFORM=browser webpack --env.mode development",
    "serve:dev": "cross-env NODE_ENV=development PLATFORM=node node server",
    "build:test": "cross-env NODE_ENV=test PLATFORM=browser webpack --env.mode production",
    "serve:test": "cross-env NODE_ENV=test PLATFORM=node node server",
    "serve:debug": "cross-env NODE_ENV=development PLATFORM=node nodemon --inspect=localhost:9000 server",
    "build:ci": "cross-env CI=true NODE_ENV=production PLATFORM=browser GENERATE_SOURCEMAP=false webpack --env.mode production",
    "serve:ci": "cross-env CI=true NODE_ENV=production PLATFORM=node node server",
    "analyze": "npm run build -- --env.presets noMinimize --env.presets analyze",
    "test": "cross-env PLATFORM=node jest --passWithNoTests",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint --ext .js --ext .jsx server client app",
    "storybook": "start-storybook",
    "postinstall": "link-module-alias"
  },
  "_moduleAliases": {
    "$app": "app",
    "$modules": "app/modules",
    "$api": "app/api",
    "$data": "app/data",
    "$components": "app/modules/common/components",
    "$controls": "app/modules/common/controls",
    "$utils": "app/modules/common/utils"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charshin/tic-tac-toe.git"
  },
  "keywords": [
    "snaphunt"
  ],
  "bugs": {
    "url": "https://github.com/charshin/tic-tac-toe/issues"
  },
  "homepage": "https://tic-tac-toe.snaphunt.com",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-do-expressions": "^7.5.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-react-jsx-source": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@emotion/babel-preset-css-prop": "^10.0.27",
